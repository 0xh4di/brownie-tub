{
  "name": "brownie-tub",
  "version": "0.0.3",
  "main": "public/main.js",
  "private": true,
  "homepage": "./",
  "build": {
    "productName": "BROWNIE TUB",
    "appId": "BrownieTub",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": "NSIS",
      "publish": [
        "github"
      ]
    }
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "electron": "^7.1.10",
    "electron-builder": "^22.3.2",
    "electron-rebuild": "^1.10.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "spectron": "^9.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.9.0",
    "node-sass": "^4.13.1",
    "pouchdb-adapter-node-websql": "^7.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "rxdb": "^8.7.5",
    "rxjs": "^6.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "rebuild": "electron-rebuild -f -w pouchdb-adapter-node-websql",
    "electron-start": "electron .",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/main.js",
    "release": "electron-builder -c.extraMetadata.main=build/main.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

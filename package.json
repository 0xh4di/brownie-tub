{
  "name": "brownie-tub",
  "version": "0.0.1",
  "description": "BROWNIE TUB web shell client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon --exec 'electron .'",
    "reset": "git reset --hard",
    "test": "mocha --timeout 20000 \"./test/**/*test-*.js\"",
    "build": "electron-builder",
    "release": "electron-builder"
  },
  "build": {
    "productName": "BROWNIE TUB",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": "NSIS",
      "publish": [
        "github"
      ]
    }
  },
  "author": "cybersecmoo",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "electron": "^7.1.10",
    "electron-builder": "^22.3.2",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "spectron": "^9.0.0"
  },
  "dependencies": {}
}
